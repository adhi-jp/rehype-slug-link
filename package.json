{
  "name": "rehype-slug-link",
  "version": "1.0.0",
  "description": "A rehype plugin that converts custom link syntax to heading links",
  "author": "adhi-jp",
  "license": "MIT",
  "keywords": [
    "rehype",
    "plugin",
    "heading",
    "link",
    "slug"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/adhi-jp/rehype-slug-link.git"
  },
  "bugs": {
    "url": "https://github.com/adhi-jp/rehype-slug-link/issues"
  },
  "homepage": "https://github.com/adhi-jp/rehype-slug-link#readme",
  "type": "module",
  "main": "index.js",
  "types": "index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "import": "./index.js"
    }
  },
  "files": [
    "index.js",
    "index.d.ts",
    "lib/"
  ],
  "packageManager": "pnpm@10.11.1",
  "scripts": {
    "check": "tsc --noEmit",
    "check:strict": "tsc --noEmit --skipLibCheck false",
    "format": "prettier --write .",
    "test": "vitest run",
    "test:watch": "vitest",
    "coverage": "vitest run --coverage",
    "prepare": "husky"
  },
  "devDependencies": {
    "@types/hast": "^3.0.4",
    "@types/unist": "^3.0.3",
    "@vitest/coverage-v8": "^3.2.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.0",
    "prettier": "^3.5.3",
    "rehype": "^13.0.2",
    "rehype-slug": "^6.0.0",
    "typescript": "^5.8.3",
    "unified": "^11.0.5",
    "vitest": "^3.2.3"
  },
  "dependencies": {
    "github-slugger": "^2.0.0",
    "unist-util-visit": "^5.0.0"
  }
}
